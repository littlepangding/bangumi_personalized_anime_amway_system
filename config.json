{
    "preproc": {
        "user_review_cnt_lo": 10,
        "user_review_cnt_hi": 1000,
        "show_review_cnt_lo": 100,
        "show_review_cnt_hi": null,
        "in_user_split_ratio": "75:15:10",
        "out_user_split_ratio": "75:15:10",
        "user_id_exclude_paired_show": true
    },
    "model": {
        "sparse": {
            "embed_dim": 396,
            "max_id": 21000,
            "pooling": "sum",
            "share_embed": true
        },
        "over": "2048-1024",
        "sparse_inter": [
            "cat",
            "dot"
        ]
    },
    "train": {
        "max_epoch": 50,
        "weight_decay_dense": 0,
        "dropout": 0.3,
        "lr": 0.01
    }
}